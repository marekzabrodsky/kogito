{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "types": ["@types/styled-components-react-native"],
    "baseUrl": "./" /* Base directory to resolve non-absolute module names. */,
    "paths": {
      "~modules/*": ["./src/modules/*"],
      "~assets/*": ["./src/assets/*"],
      "~gql/*": ["./gql/__generated__/*"]
    }
  }
}
