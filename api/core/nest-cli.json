{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/core/src",
  "monorepo": true,
  "root": "apps/core",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/core/tsconfig.app.json"
  },
  "projects": {
    "app": {
      "type": "library",
      "root": "libs/app",
      "entryFile": "index",
      "sourceRoot": "libs/app/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
