{
  "name": "core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/core/main",
    "start:backoffice": "nest start backoffice",
    "start:backoffice:dev": "nest start backoffice --watch",
    "start:backoffice:debug": "nest start backoffice --debug --watch",
    "start:backoffice:prod": "node dist/apps/backoffice/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/core/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "pretypeorm": "ts-node -r tsconfig-paths/register scripts/write-type-orm-config.ts",
    "typeorm:migration:create": "npm run typeorm -- migration:create -n",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n afterResearch",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.9.11",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.21.0",
    "bcrypt": "^5.0.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.31",
    "xmldoc": "^1.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/app/(.*)": "<rootDir>/libs/app/src/$1",
      "@app/app": "<rootDir>/libs/app/src",
      "@app/auth/(.*)": "<rootDir>/libs/auth/src/$1",
      "@app/auth": "<rootDir>/libs/auth/src",
      "@app/configuration/(.*)": "<rootDir>/libs/configuration/src/$1",
      "@app/configuration": "<rootDir>/libs/configuration/src",
      "@app/content/(.*)": "<rootDir>/libs/content/src/$1",
      "@app/content": "<rootDir>/libs/content/src",
      "@app/diary/(.*)": "<rootDir>/libs/diary/src/$1",
      "@app/diary": "<rootDir>/libs/diary/src",
      "@app/neogate/(.*)": "<rootDir>/libs/neogate/src/$1",
      "@app/neogate": "<rootDir>/libs/neogate/src",
      "@app/questionnaire/(.*)": "<rootDir>/libs/questionnaire/src/$1",
      "@app/questionnaire": "<rootDir>/libs/questionnaire/src",
      "@app/user/(.*)": "<rootDir>/libs/user/src/$1",
      "@app/user": "<rootDir>/libs/user/src"
    }
  }
}
